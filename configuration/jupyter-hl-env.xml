<configuration>
    <property>
        <name>jupyter_port</name>
        <display-name>Jupyter Port</display-name>
        <value>8085</value>
        <description>Port to run jupyter</description>
    </property>

    <property>
        <name>jupyter_host</name>
        <display-name>Jupyter Host</display-name>
        <value>root@izb-lab01</value>
        <description>Host where jupyter is running</description>
    </property>

    <property>
        <name>jupyter_password</name>
        <display-name>Jupyter Password</display-name>
        <value>headlabs</value>
        <description>Password to run jupyter</description>
    </property>

    <property>
        <name>jupyter_root_dir</name>
        <display-name>Jupyter Root Directory</display-name>
        <value>/</value>
        <description>Root directory in jupyter</description>
    </property>
</configuration>
